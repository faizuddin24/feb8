<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
    <servlet>
        <servlet-name>FormHandlerServlet</servlet-name>
        <servlet-class>FormHandlerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FormHandlerServlet</servlet-name>
        <url-pattern>/FormHandlerServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DateTimeServlet</servlet-name>
        <servlet-class>DateTimeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DateTimeServlet</servlet-name>
        <url-pattern>/DateTimeServlet</url-pattern>
    </servlet-mapping>
</web-app>
